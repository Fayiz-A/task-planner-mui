{
  "name": "task-planner-mui",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": ".",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-start": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "electron-builder -m"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.16.14",
    "@mui/x-date-pickers": "^6.0.0",
    "cra-template": "1.2.0",
    "date-fns": "^2.30.0",
    "mobx": "^6.13.5",
    "mobx-react-lite": "^4.1.0",
    "path": "^0.12.7",
    "play-sound": "^1.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.1.3",
    "react-scripts": "5.0.1",
    "react-toastify": "^11.0.3",
    "web-vitals": "^4.2.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "electron": "^34.0.0",
    "electron-builder": "^25.1.8",
    "wait-on": "^7.0.0"
  },
  "build": {
    "appId": "com.example.daily-schedule-planner",
    "productName": "Daily Schedule Planner",
    "mac": {
      "category": "public.app-category.productivity",
      "target": "dmg"
    },
    "directories": {
      "buildResources": "public"
    },
    "files": [
      "build/**/*",
      "public/**/*",
      "electron.js"
    ]
  }
}
